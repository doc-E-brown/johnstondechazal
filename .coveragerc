[run]
omit =
    tests/*
    */test*.py
    */migrations/*
    */wsgi.py
    */apps.py
    *_version.py
parallel = true

[paths]
source =
    johnstondechazal 

[report]
precision = 2
include =
    johnstondechazal/*.py
exclude_lines =
    def __str__
    pragma: no cover

[html]
directory = coverage_html_report
